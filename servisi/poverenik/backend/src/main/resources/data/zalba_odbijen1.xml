    mesto_podnosenja_zalbe="Sabac" datum_podnosenja_zalbe="2020-01-01" datum_podnosenja_zahteva="2020-01-01">
    <zoz:Podaci_o_primaocu>
        <tipovi:Naziv property="pred:primalacNaziv">Primalac</tipovi:Naziv>
        <tipovi:Sediste>
            <tipovi:Mesto>Mesto Primaoca</tipovi:Mesto>
            <tipovi:Ulica>Ulica primaoca</tipovi:Ulica>
            <tipovi:Ulicni_broj>1</tipovi:Ulicni_broj>
        </tipovi:Sediste>
    </zoz:Podaci_o_primaocu>
    
    <zoz:Podaci_o_zaliocu>
        <tipovi:Ime property="pred:podnosilacIme">Zalilac</tipovi:Ime>
        <tipovi:Prezime property="pred:podnosilacPrezime">Zaliocic</tipovi:Prezime>
        <tipovi:Adresa>
            <tipovi:Mesto>Mesto Zalioca</tipovi:Mesto>
            <tipovi:Ulica>Ulica zalioca</tipovi:Ulica>
            <tipovi:Ulicni_broj>1</tipovi:Ulicni_broj>
        </tipovi:Adresa>
    </zoz:Podaci_o_zaliocu>
    
    <zoz:Podaci_o_odluci broj_odluke="0" godina="0">
        <zoz:Naziv_donosioca_odluke>Donosioc odluke</zoz:Naziv_donosioca_odluke>
    </zoz:Podaci_o_odluci>
    
    <zoz:Telo_zalbe>
        <zoz:Razlog_zalbe>Neprofesionilazam uzeli ste mi 10 poena</zoz:Razlog_zalbe>
        <zoz:Osnova_zalbe>
            <zoz:Clan></zoz:Clan>
            <zoz:Stav></zoz:Stav>
            <zoz:Zakon></zoz:Zakon>
        </zoz:Osnova_zalbe>
    </zoz:Telo_zalbe>
    
    <zoz:Podaci_o_podnosiocu_zalbe>
        <tipovi:Ime property="pred:podnosilacIme">Podnosioc</tipovi:Ime>
        <tipovi:Prezime property="pred:podnosilacPrezime">Podnosiocic</tipovi:Prezime>
        <tipovi:Adresa>
            <tipovi:Mesto>Mesto Podnosioca</tipovi:Mesto>
            <tipovi:Ulica>Adresa podnosioca</tipovi:Ulica>
            <tipovi:Ulicni_broj>1</tipovi:Ulicni_broj>
        </tipovi:Adresa>
        <tipovi:Kontakt_podaci></tipovi:Kontakt_podaci>
    </zoz:Podaci_o_podnosiocu_zalbe>
    
</zoz:Zalba_odbijanje>